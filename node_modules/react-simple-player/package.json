{
  "name": "react-simple-player",
  "version": "1.1.0",
  "description": "",
  "author": {
    "name": "streamich",
    "url": "https://github.com/streamich"
  },
  "homepage": "https://github.com/streamich/react-simple-player",
  "repository": "streamich/react-simple-player",
  "license": "Unlicense",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rimraf public/dist",
    "build": "tsc",
    "test": "jest --no-cache --config='jest.config.js'",
    "start": "yarn storybook",
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:diff": "prettier -l 'src/**/*.{ts,tsx,js,jsx}'",
    "prepush": "yarn prettier:diff",
    "precommit": "pretty-quick --staged && yarn tslint",
    "tslint": "tslint 'src/**/*.{js,jsx,ts,tsx}' -t verbose",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "release": "semantic-release",
    "storybook": "start-storybook -p 6010",
    "storybook:build": "build-storybook",
    "storybook:clean": "rimraf storybook-static"
  },
  "keywords": [],
  "dependencies": {
    "p4-css": "*",
    "react-use": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@storybook/react": "^5.3.18",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.7",
    "storybook-readme": "^5.0.8",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tslint": "^6.1.1",
    "tslint-config-common": "^1.6.0",
    "typescript": "^3.8.3"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tslint",
      "pre-push": "yarn tslint && yarn clean && yarn build && yarn test"
    }
  }
}
